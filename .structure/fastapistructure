; our_project/
; │
; └───app/
; │   │   main.py
; │   │   models.py
; │   │   schemas.py
; │   │   crud.py
; │   │   dependencies.py
; │   │   database.py  # Defines database session and engine
; │   
; └───requirements.txt

; basic structure



; our_project/
; │
; ├── app/                        # Main application directory
; │   ├── __init__.py             # Makes app a Python module
; │   │
; │   ├── main.py                 # Entry point to your FastAPI application
; │   │
; │   ├── dependencies.py         # Dependency injection (e.g., get_db)
; │   │
; │   ├── aws_utils.py            # AWS utilities, including async S3 interactions
; │   │
; │   ├── database.py             # Database configurations
; │   │
; │   ├── models.py               # SQLAlchemy models
; │   │
; │   ├── schemas.py              # Pydantic models for request/response validation
; │   │
; │   ├── crud.py                 # CRUD operations
; │   │
; │   └── modules/                # Directory for specific functionalities/modules
; │       ├── data_cleaner_utils_page1.py  # Asynchronous file processing utilities
; │       ├── keypress_decoder_utils_page3.py  # Utilities for decoding keypresses
; │       └── questionnaire_utils_page2.py     # Utilities for questionnaire handling
; │
; ├── tests/                      # Test suite
; │   ├── __init__.py
; │   └── test_main.py
; │
; ├── dockerfile                  # Docker configuration for deployment
; ├── requirements.txt            # Project dependencies
; └── .env                        # Environment variables (not tracked by git)
